includes:
  - vendor/szepeviktor/phpstan-wordpress/extension.neon

parameters:
  level: 5
  paths:
    - functions.php
    - includes/
    - src/
    - app/
  
  autoload_files:
    - autoload.php

  bootstrapFiles:
    - vendor/autoload.php

  ignoreErrors:
    - ''

  reportUnmatchedIgnoredErrors: true

  memoryLimit: 512M
